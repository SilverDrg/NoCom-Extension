{
  "short_name": "NoCom",
  "name": "NoCom",
  "description": "The power of React and TypeScript for building interactive Chrome extensions",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
      "default_popup": "index.html",
      "default_title": "Open NoCom"
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
      "16": "icon-light16.png",
      "48": "icon-light48.png",
      "128": "icon-light128.png"
  }
}
